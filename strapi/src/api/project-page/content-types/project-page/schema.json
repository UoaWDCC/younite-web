{
  "kind": "collectionType",
  "collectionName": "project_pages",
  "info": {
    "description": "",
    "displayName": "Projects",
    "pluralName": "project-pages",
    "singularName": "project-page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "title": {
      "required": true,
      "type": "string"
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "content-block.centered-text-block",
        "content-block.content-block",
        "content-block.centered-paragraph",
        "content-block.rich-text"
      ]
    },
    "Date": {
      "type": "date",
      "required": true,
      "default": "2023-09-07"
    },
    "Description": {
      "type": "text",
      "required": true
    }
  }
}
